{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "blocklayouts/icon",
  "version": "0.1.0",
  "title": "Icon",
  "category": "design",
  "icon": "star-filled",
  "description": "Add a customizable icon to your content.",
  "supports": {
    "reusable": false,
    "html": false,
    "align": [
      "wide",
      "full"
    ],
    "color": {
      "__experimentalSkipSerialization": true,
      "gradients": true,
      "link": false,
      "__experimentalDefaultControls": {
        "background": true,
        "text": true
      }
    },
    "__experimentalBorder": {
      "__experimentalSkipSerialization": true,
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "spacing": {
      "__experimentalSkipSerialization": true,
      "margin": true,
      "padding": true,
      "__experimentalDefaultControls": {
        "margin": false,
        "padding": true
      }
    },
    "shadow": {
      "__experimentalSkipSerialization": true
    }
  },
  "attributes": {
    "tagName": {
      "type": "string",
      "default": "div"
    },
    "iconName": {
      "type": "string",
      "default": "wordpress"
    },
    "customSvgCode": {
      "type": "string"
    },
    "iconType": {
      "type": "string",
      "default": "fill"
    },
    "strokeWidth": {
      "type": "number",
      "default": 1.2
    },
    "iconSize": {
      "type": "number",
      "default": 60
    },
    "width": {
      "type": "number"
    },
    "justifyContent": {
      "type": "string"
    }
  },
  "textdomain": "blocklayouts-icon",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}